COUNT=1
[ $COUNT ];
cleaner()
{
rm -rf /data/user/0/com.vng.pubgmobile/shared_prefs &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/no_backup &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/files &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/databases &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/cache &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_crashrecord &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_bugly &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_geolocation &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_webview_imsdk_inner_webview &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_webview &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_textures &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_tbs &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_databases &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/app_appcache &> /dev/null
rm -rf /data/user/0/com.vng.pubgmobile/code_cache &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/StatEventReportedFlag &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/GameErrorNoRecords &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/ODPaks/ &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/UpdateInfo/apollo_uuid_define.json &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/UpdateInfo/apollo_loglist.json &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/RoleInfo &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/PufferTmpDir &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Pandora &> /dev/null
rm -rf /storage/emulated/0/Android/data/com.vng.pubgmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/1950038955_3090_0.16.5.11560_20200108020124_1155095217_cures.ifs.res &> /dev/null
