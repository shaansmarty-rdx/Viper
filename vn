rm -rf /storage/emulated/0/.backups &> /dev/null
rm -rf /storage/emulated/0/tencent &> /dev/null
#echo "Cleaning = $COUNT at $( date +"%H:%M:%S" )"
let COUNT=COUNT+1
} 
PACKAGE='com.vng.pubgmobile'
while [ $(pidof $PACKAGE) ]
do
cleaner
if [ ! $(pidof $PACKAGE) ]; then
break
fi
done
sleep 2
echo "" 
echo "" 
echo "Game stopped, Resetting all Files" 
echo ""
sleep 1
((final=$COUNT+1))
printf " █ NUMBER OF TIME CLEARED █---------> ";
echo $final
echo "" 
sleep 1
((report=$COUNT/500))
printf "Number of report Stopped = ";
echo $report
echo "" 
printf ">>>From All Reports we Finally stop ";
echo $report "Report" 
