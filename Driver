#!/bin/sh
show_menu(){
normal=`echo "\033[m"`
    menu=`echo "\033[36m"` #Blue
    number=`echo "\033[33m"` #yellow
    bgred=`echo "\033[41m"`
    fgred=`echo "\033[31m"`
YELLOW='\033[1;33m'
printf "${fgred}         © 尺ᗪ乂 尸尺龱丿㠪⼕ㄒ ᐯ工尸㠪尺 ©      \n"
printf "${menu}   ♛ A̲ C̲o̲m̲p̲l̲e̲t̲e̲l̲y̲ A̲u̲t̲o̲m̲a̲t̲e̲d̲ P̲r̲o̲j̲e̲c̲t̲ ♛         "
cat /data/data/com.termux/files/home/Viper/pytransform/Trident | lolcat -a -d 5
   ./Header
    
    printf "\n\n${menu}████████████████████████████████████████████████████████${normal}\n"
    printf "\n${menu}██${number} 1)${menu} 𝗕𝗬𝗣𝗔𝗦𝗦 𝗞𝗢𝗥𝗘𝗔 ${normal}\n\n" | lolcat -a -d 3
    printf "${menu}██${number} 2)${menu} 𝗕𝗬𝗣𝗔𝗦𝗦 𝗚𝗟𝗢𝗕𝗔𝗟 ${normal}\n\n" | lolcat -a -d 3
    printf "${menu}██${number} 3)${menu} 𝗖𝗟𝗘𝗔𝗡 𝗟𝗢𝗚𝗦 ${normal}\n\n" | lolcat -a -d 3
    printf "${menu}██${number} 4)${menu} 𝗥𝗘𝗦𝗘𝗧 𝗚𝗨𝗘𝗦𝗧 ${normal}\n\n" | lolcat -a -d 3
    printf "${menu}██${number} 5)${menu} 𝗖𝗛𝗔𝗡𝗚𝗘 𝗗𝗡𝗦 𝘁𝗼 𝟴.𝟴.𝟴.𝟴${normal}\n\n" | lolcat -a -d 3
    printf "${menu}████████████████████████████████████████████████████████${normal}\n"
    printf "\n${fgred}Please enter a menu option and enter or ${fgred}x to exit. ${normal}"
    read opt
}

option_picked(){
    msgcolor=`echo "\033[01;31m"` # bold red
    normal=`echo "\033[00;00m"` # normal white
    message=${@:-"${normal}Error: No message passed"}
    printf "${msgcolor}${message}${normal}\n"
}

